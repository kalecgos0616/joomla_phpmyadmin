<?xml version="1.0" encoding="utf-8"?>
<extension version="2.5" type="plugin" group="editors-xtd" method="upgrade">
	<name>Button - MyJspace Tags</name>
	<creationDate>01/06/2014</creationDate>
	<author>Bernard Saulme</author>
	<authorEmail>bernard@wanadoo.fr</authorEmail>
	<authorUrl>http://softs.saulme.fr/</authorUrl>
	<copyright>Version by Bernard Saulme</copyright>
	<license>GNU/GPL</license>
	<version>2.4.0</version>
	<build>14</build>

	<description>PLG_EDITORSXTD_TAGSMYJSPACE_XML_DESCRIPTION</description>

	<scriptfile>install.php</scriptfile>

	<files>
		<filename plugin="tagsmyjspace">tagsmyjspace.php</filename>
		<filename>index.html</filename>
		<filename>tagsmyjspace.css</filename>
		<filename>tags.png</filename>
		<filename>tags-16.png</filename>
		<filename>COPYRIGHT.php</filename>
	</files>

	<languages folder="language">
		<language tag="en-GB">en-GB.plg_editors-xtd_tagsmyjspace.ini</language>
		<language tag="en-GB">en-GB.plg_editors-xtd_tagsmyjspace.sys.ini</language>
	</languages>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="class_tagmyjspace" type="text" default="tagsmyjspace" label="PLG_EDITORS_XTD_TAGSMYJSPACE_CLASS_MYJSPACE_LABEL" description="PLG_EDITORS_XTD_TAGSMYJSPACE_CLASS_MYJSPACE_DESC" />
			</fieldset>
		</fields>
	</config>
</extension>
